{
  "name": "tokiyori_devkit",
  "version": "1.0.0",
  "description": "TOKIYORI standard front-end development kit",
  "main": "",
  "scripts": {
    "css": "run-s css:scss css:postcss",
    "css:scss": "sass ./assets/scss/:../assets/css/ --no-source-map",
    "css:postcss": "postcss ../assets/css/style.css -u css-mqpacker autoprefixer -o ../assets/css/style.css",
    "watch:css": "watch 'npm run css' ./assets/scss/",
    "watch:server": "browser-sync start --server ../ --files='../**/*.html, ../assets/**/*.css, ../assets/**/*.js' --startPath='../' ",
    "serve": "npm-run-all -p watch:*",

    "build": "NODE_ENV=production webpack --mode production",
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "sass-ver": "sass --version",
    "start": "webpack serve --mode development"
  },
  "author": "KousukeBanzai",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "autoprefixer": "^9.8.7",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.5",
    "browser-sync-webpack-plugin": "^2.3.0",
    "css-loader": "^6.3.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.11",
    "fibers": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.8",
    "postcss-cli": "^7.1.2",
    "postcss-loader": "^6.1.1",
    "postcss-sort-media-queries": "^4.1.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "watch": "^1.0.2",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.8.0"
  },
  "browserslist": [
    ">0.25% in JP",
    "not ie <= 10",
    "not op_mini all"
  ],
  "dependencies": {
    "jquery": "^3.6.0",
    "vue": "^2.6.14",
    "vuelidate": "^0.7.6"
  }
}
